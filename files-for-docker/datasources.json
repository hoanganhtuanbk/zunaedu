{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "Email": {
    "name": "Email",
    "connector": "mail",
    "transports": [
      {
        "type": "smtp",
        "host": "smtp.gmail.com",
        "secure": true,
        "port": 465,
        "tls": {
          "rejectUnauthorized": false
        },
        "auth": {
          "user": "${email}",
          "pass": "${password}"
        }
      }
    ]
  },
  "mongo": {
    "name": "mongo",
    "connector": "mongodb",
    "url": "mongodb://mongo:27017/paas"
  }
}
